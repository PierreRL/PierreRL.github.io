{
    "compilerOptions": {
        "skipLibCheck": true,
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "strict": true,
        "esModuleInterop": true,
        "paths": {
            "src/*": [
                "./src/*"
            ],
        }
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "./src/**/*.ts"
    ]
}